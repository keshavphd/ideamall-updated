{
    "version":2,
    "name":"IdeaMall",
    "builds":[
        {
        "src":"/*.js",
        "use":"@vercel/node"
        }
    ],
    "routes":[
        {
           "src":"/(.*)",           
           "dest":"/"
        }
    ],
    "rewrites":[
        {
            "source":"/api/(.*)",
            "destination":"/api"
        }
    ]
}
